{
	"env": {
			"browser": true,
			"es2021": true
	},
	"extends": "airbnb-base/legacy",
	"parserOptions": {
			"ecmaVersion": 12,
			"isImpliedStrict": true,
			"sourceType": "module"
	},
	"rules": {
			"quotes": ["error", "single"],
			"semi": ["error", "always"],
			"eqeqeq": ["error", "always"],
			"arrow-body-style": ["error", "always"],
      "no-var": "error"
	}
}
